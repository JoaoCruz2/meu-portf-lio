                },
                success: function(response) {
                    window.location = "https://qr-code.io/pt/pdf-download?file=" + response.data.path + "&fileName=" + response.data.fileName;
                },
                error: (response) => {
                    console.log(response);
                }
            });
        }
        reCorrectSvg(link).then((res) => {
            image.src = URL.createObjectURL(new Blob([res], {
